{
  "productName": "Todo Notes Tracker",
  "version": "1.2.3",
  "identifier": "com.todonotestracker",
  "build": {
    "frontendDist": "../ui"
  },
  "app": {
    "windows": [
      {
        "title": "Todo Notes Tracker",
        "width": 900,
        "height": 700,
        "minWidth": 400,
        "minHeight": 300,
        "resizable": true,
        "devtools": true
      }
    ],
    "security": {
      "csp": null
    },
    "withGlobalTauri": true
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png", 
      "icons/icon.png",
      "icons/icon.ico"
    ]
  },
  "plugins": {}
}